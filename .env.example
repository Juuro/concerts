# Contentful
CONTENTFUL_SPACE_ID=your_space_id_here
CONTENTFUL_DELIVERY_TOKEN=your_delivery_token_here

# OpenCage API
OPENCAGE_API_KEY=your_opencage_key_here

# Last.fm API (optional)
LASTFM_API_KEY=your_lastfm_key_here
LASTFM_SECRET=your_lastfm_secret_here

# Feature Flags
# Set to 'true', '1', or 'yes' to enable, anything else to disable
# When ENABLE_LASTFM is disabled, no Last.fm API calls are made and no Last.fm data is included
ENABLE_LASTFM=true
# When ENABLE_GEOCODING is disabled, no geocoding API calls are made and coordinates are displayed instead of city names
ENABLE_GEOCODING=true
