    "better-auth": "^1.4.18",
    "contentful": "^11.10.2",
    "lastfm-ts-api": "^1.0.0",
    "leaflet": "^1.9.3",
    "leaflet.markercluster": "^1.5.3",
    "next": "^15.1.4",
