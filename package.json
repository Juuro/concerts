    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "sharp": "^0.33.5",
    "standard-version": "^9.0.0"
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.8",
    "@types/leaflet.markercluster": "^1.5.4",
    "@types/node": "^22.10.2",
    "@types/pg": "^8.16.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "eslint": "^9",
    "eslint-config-next": "^15.1.4",
    "prettier": "^3.5.3",
    "sass": "^1.86.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.7.2"
  },
  "keywords": [
    "nextjs",
    "concerts"
  ],
  "license": "MIT",
  "scripts": {
    "build": "prisma generate && next build",
    "dev": "prisma generate && next dev",
    "start": "next start",
    "format": "prettier --write ./src/**/*.{js,jsx,ts,tsx,scss}",
    "lint": "next lint",
    "release": "standard-version",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "migrate:contentful": "tsx -r tsconfig-paths/register scripts/migrate-contentful.ts",
    "migrate:venues": "node --env-file=.env.local ./node_modules/.bin/tsx scripts/migrate-venue-data.ts",
    "backfill:city": "tsx -r tsconfig-paths/register scripts/backfill-city.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Juuro/concerts"
  },
